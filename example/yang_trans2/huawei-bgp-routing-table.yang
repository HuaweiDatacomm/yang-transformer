   typedef region-validation-result-type {
     type "enumeration" {
       enum "valid" {
         value "1";
         description
           "The BGP region validation result is valid.";
       }
       enum "invalid" {
         value "2";
         description
           "The BGP region validation result is invalid.";
       }
     }
