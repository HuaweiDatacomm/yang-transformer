   ext:task-name "bgp";
   typedef bgp-origin-type {
     type "enumeration" {
       enum "igp" {
         value "1";
         description
           "IBGP route.";
       }
       enum "egp" {
         value "2";
         description
           "EBGP route.";
       }
       enum "incomplete" {
         value "3";
         description
           "Route whose source is unknown.";
       }
     }
     description
       "The origin of bgpcommon.";
   }
   typedef flapping-af-type {
     type "enumeration" {
       enum "ipv4uni" {
         value "1";
         description
           "IPv4 unicast address family.";
       }
       enum "ipv4multi" {
         value "2";
         description
           "IPv4 multicast address family.";
       }
       enum "ipv4vpn" {
         value "3";
         description
           "IPv4 VPN address family.";
       }
       enum "ipv4labeluni" {
         value "4";
         description
           "IPv4 labeled address family.";
       }
       enum "ipv6uni" {
         value "5";
         description
           "IPv6 unicast address family.";
       }
       enum "ipv6vpn" {
         value "6";
         description
           "IPv6 VPN address family.";
       }
     }
     description
       "The address family type of flapping.";
   }
   typedef bgp-external-flag-type {
     type "bits" {
       bit type5-ip-prefix {
         position "5";
         description
           "Ip prefix.";
       }
       bit type2-irb {
         position "13";
         description
           "IRB.";
       }
     }
     description
       "Route external flags.";
   }
   typedef hierarchy-convergence-type {
     type "enumeration" {
       enum "base" {
         value "1";
