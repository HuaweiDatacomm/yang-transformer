             key "vpn-name route-distinguisher prefix mask-length route-type "+
                 "im-table-id from path-id";
             description
               "Statistics of routes.";
             leaf vpn-name {
               type "leafref" {
                 path "/ni:network-instance/ni:instances/ni:instance/ni:name";
               }
               description
                 "VPN name of a BGP instance.";
             }
             leaf route-distinguisher {
               type "string" {
                 length "1..22";
               }
               description
                 "Route distinguisher.";
             }
             leaf prefix {
               type "inet:ip-address-no-zone";
