   namespace "urn:huawei:yang:huawei-bgp-routing-table";
   prefix "bgp-rt";
   import huawei-bgp {
     prefix "bgp";
   }
   import huawei-extension {
     prefix "ext";
   }
   import ietf-inet-types {
     prefix "inet";
   }
   import huawei-pub-type {
     prefix "pub-type";
