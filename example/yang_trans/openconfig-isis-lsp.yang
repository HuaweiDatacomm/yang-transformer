 submodule openconfig-isis-lsp {
   belongs-to openconfig-isis {
     prefix "oc-isis";
   }
   import openconfig-extensions {
     prefix "oc-ext";
   }
   // meta

   organization
     "OpenConfig working group";
   contact
     "OpenConfig working group\nwww.openconfig.net ";
